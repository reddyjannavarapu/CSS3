<style>
    .scroll {
        max-height: 360px;
        overflow: auto;
    }
</style>
<div id="modal-form_Individual" class="modalIndividual modal row" tabindex="-1">
    <div class="modal-dialog" style="left: 20%;">
        <div class="modal-content" style="width: 150%; height: 1000px">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="blue bigger"></h4>
            </div>
            <div class="modal-body">

                <div class="row">
                    <div style="margin-left: 10px;" class="col-xs-4 no-padding-top no-padding-left">
                        <div style="padding-top: 62px; padding-bottom: 20px; margin-top: 10px;" class="col-xs-12">

                            <div class="col-xs-8" style="font-size: 19px;">
                                <span class="text-left">TAX INVOICE</span>
                            </div>
                            <div class="col-xs-12">
                                <label id="lblCompanyName"></label>
                                <label id="lblAddress1"></label>
                                <label id="lblAddress2"></label>
                                <label id="lblAddress3"></label>

                            </div>
                        </div>

                    </div>

                    <div style="padding-top: 14px; padding-bottom: 20px; margin-top: 10px; margin-left: 273px;" class="col-xs-4 no-padding-top no-padding-left">

                        <div class="text-left col-xs-12" style="font-size: 20px;">
                            FMG CORPORATE SERVICES PTE LTD
                        </div>
                        <div class="col-xs-12 margin-10t">
                            8 Wilkie Road
                            <br />
                            #03-01, Wilkie Edge
                            <br />
                            Singapore 228095
                        </div>

                        <div class="text-left col-xs-12" style="padding-top: 10px;">
                            <table>
                                <tr>
                                    <td>Telephone</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">(65) 6533 7600</td>
                                </tr>
                                <tr>
                                    <td>Facsimile</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">(65) 6594 7855</td>
                                </tr>
                                <tr>
                                    <td>Email</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">ca@fmg.com.sg</td>
                                </tr>
                            </table>                            
                        </div>
                        <div class="col-xs-12">
                        </div>
                        <div class="text-left col-xs-12" style="padding-top: 10px;">
                            <table>
                                <tr>
                                    <td>Page </td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">
                                        <label> 1 of 1</label>
                                    </td>
                                </tr>
                                <tr>
                                    <td> Date  </td>
                                    <td class="padding-10l">:</td>
                                    <td class="padding-10l">
                                        <label> @DateTime.Now.ToString("dd MMM yyyy")</label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>Tax Invoice No. </td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">
                                        <label> <span id="spnTaxInvoiceNo"></span></label>
                                    </td>
                                </tr>
                                <tr>
                                    <td> GST Reg No. </td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">
                                        <label> M2-0112785-8 </label>
                                    </td>
                                </tr>
                            </table>
                        </div>


                    </div>
                </div>
                <div class="tableGrid dataTables_wrapper ace-scroll" @*style="max-height: 300px; overflow-y: auto;"*@>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th style="width: 500px">DESCRIPTION</th>
                                <th style="width: 128px">ACCOUNTS CODE</th>
                                <th>SGD</th>
                            </tr>
                        </thead>
                        <tbody id="tblPreviewData" class="scroll-content" style="max-height: 300px;">
                            <tr></tr>
                        </tbody>


                    </table>


                </div>
                <div style="padding-left: 600px;">
                    Total :
                    <label class="lblTotalAmount"></label>
                </div>
                <div class="row">
                    <div style="float: left" class="col-xs-6 ">
                        <div style="padding-top: 62px; padding-bottom: 20px; margin-top: 10px; width: 350px" class="col-xs-12">
                            <table>
                                <tr>
                                    <td colspan="3">Bank Details</td>

                                </tr>
                                <tr>
                                    <td>Payable To</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">FMG CORPORATE SERVICES PTE LTD</td>
                                </tr>

                                <tr>
                                    <td>Account To</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">010-140673-8</td>
                                </tr>

                                <tr>
                                    <td>Bank Names</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">Standard Chartered Bank</td>
                                </tr>


                                <tr>
                                    <td>Bank Address</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">6 Battery Road Singapore 049909</td>
                                </tr>


                                <tr>
                                    <td>Bank Code</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">7144</td>
                                </tr>

                                <tr>
                                    <td>Branch Code</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">001</td>
                                </tr>

                                <tr>
                                    <td>Bank Swift</td>
                                    <td class="padding-10l"> : </td>
                                    <td class="padding-10l">SCBLSG22</td>
                                </tr>
                                <tr>
                                    <td colspan="3">All bank charges are to be borne by the remitter.</td>

                                </tr>
                            </table>
                        </div>
                    </div>
                    <div style="padding-top: 85px; float: left" class="col-xs-5">

                        <div class="text-left col-xs-12" style="font-size: 14px;">
                            FMG CORPORATE SERVICES PTE LTD
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="scripts">
    <script id="scriptInvoice" type="text/html">
        <tr>
            <td>
                <label class="lblDescription">{{= Description}}</label>
            </td>
            <td>{{= ACCPACCode}}</td>
            <td style="text-align: right ! important; padding-right: 20%;">{{= Amount}}</td>

        </tr>
    </script>
</div>
<script src="~/Areas/Billing/Content/Scripts/_InvoicePreviewData.js"></script>
