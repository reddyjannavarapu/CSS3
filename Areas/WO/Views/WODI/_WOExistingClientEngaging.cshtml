<style>
    .ui-datepicker {
        width: 19em;
    }

    .chosen-container {
        width: auto !important;
    }
</style>


<div class="row">
    <div class="col-xs-12">
        @Html.Partial("~/Areas/WO/Views/Css1Info/_Css1Info.cshtml", new ViewDataDictionary { { "PopupHeader", "CSS1 Existing Client Engaging Info" } })
    </div>
</div>

<div class="row margin-10t">

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 20px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Currency</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <select id="ddlCurrency" tabindex="1" class="col-xs-8">
                        <option value="-1">Select</option>
                    </select>
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Class Of Share</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <select id="ddlClassOfShare" tabindex="2" class="col-xs-8">
                        @*<option value="-1">Select</option>*@
                    </select>
                </div>

                <div class="col-xs-11 margin-10t">
                    <span class="text-left">Total No Of New Shares To Be Allotted</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtAllotedShares" onkeypress="return EnterOnlyNumbers(event)" placeholder="Total No Of New Shares To Be Allotted" class="form-control col-xs-8 DisableCutCopyPaste" maxlength="30" tabindex="3" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Consideration Of Each Share</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtConsiderationOfEachShare" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Consideration Of Each Share" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="4" />
                </div>

                <div class="col-xs-11 margin-10t">
                    <span class="text-left">Amount To Be Treated As Paid On Each Share</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtPaidOnEachShare" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Amount To Be Treated As Paid On Each Share" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="5" />
                </div>

                <div class="col-xs-12 margin-10t">
                    <span class="text-left">Total Consideration</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" id="txtTotalConsideration" placeholder="Total Consideration" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="6" />
                </div>

                <div class="col-xs-12 margin-10t">
                    Resultant Total No Of Issued Shares
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" onkeypress="return EnterOnlyNumbers(event)" placeholder="Resultant Total No Of Issued Shares" id="txtTotalIssuedShares" maxlength="30" class="form-control col-xs-8 DisableCutCopyPaste" tabindex="7" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Resultant Issued Capital
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Resultant Issued Capital" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" id="txtResultantIssuedCapital" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="8" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Resultant Paid Up Capital
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Resultant Issued Capital" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" id="txtResultantPaidUpCapital" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="9" />
                </div>

            </div>

        </div>

    </div>

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 250px;">

            <div class="col-xs-offset-1">

                <div class="text-left col-xs-12 margin-10t" style="display: none;">
                    Share Holding Structure
                </div>
                <div class="col-xs-12 margin-2t" style="display: none;">
                    <select id="ddlShareHoldingStructure" tabindex="10" class="col-xs-8">
                        <option value="-1">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-11 margin-10t">
                    <div class="col-xs-9 no-padding-left">
                        RO as Place of Meeting
                    </div>
                    <div class="col-xs-2 no-padding-left">
                        <label>
                            <input type="checkbox" id="chkROPlaceOfMeeting" tabindex="11" class="col-xs-9 ace" />
                            <span class="lbl"></span>
                        </label>
                    </div>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Line1
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Address Line1" id="txtMeetingAddressLine1" class="col-xs-9 special" maxlength="100" tabindex="12" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Line2
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Address Line2" id="txtMeetingAddressLine2" class="col-xs-9 special" maxlength="100" tabindex="13" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Line3
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtMeetingAddressLine3" placeholder="Meeting Address Line3" class="col-xs-9 special" maxlength="100" tabindex="14" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Country
                </div>
                <div class="col-xs-11 margin-2t">
                    <select class="col-xs-10 no-padding-left" id="ddlMeetingAddressCountry" tabindex="15">
                        <option value="-1">Select</option>
                    </select>
                </div>
                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Postal Code
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Address Postal Code" id="txtMeetingAddressPostalCode" maxlength="10" class="col-xs-9 DisableCutCopyPaste" tabindex="16" />
                </div>

            </div>

        </div>

    </div>

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 176px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-12 margin-10t">
                    <h4 class="text-left  col-xs-12">Signing Director</h4>
                </div>

                <div class="col-xs-12 margin-10t">

                    <div class="col-xs-8 margin-10t">
                        <span class="text-left">Meeting Notice</span>
                    </div>
                    <div class="col-xs-12 margin-2t" tabindex="17">
                        @Html.Partial("~/Areas/WO/Views/PartialContent/_DirectorsChosen.cshtml", new ViewDataDictionary { { "Id", "MeetingNotice" }, { "selectedId", "" }, { "SearchType", "Officers" }, { "SearchRole", "GEN" }, { "WOID", @ViewBag.WOID }, { "DefaultLoad", 1 }, { "AutoSearch", 1 }, { "NatureAppoint", "7,9" }, { "DisplayName", "Meeting Notice" } })
                    </div>

                    <div class="col-xs-8 margin-10t">
                        <span class="text-left">Meeting Minutes</span>
                    </div>
                    <div class="col-xs-12 margin-2t" tabindex="18">
                        @Html.Partial("~/Areas/WO/Views/PartialContent/_DirectorsChosen.cshtml", new ViewDataDictionary { { "Id", "MeetingMinutes" }, { "selectedId", "" }, { "SearchType", "MeetingMinutes" }, { "SearchRole", "GEN" }, { "WOID", @ViewBag.WOID }, { "DefaultLoad", 1 }, { "AutoSearch", 1 }, { "DisplayName", "Meeting Minutes" } })
                    </div>

                    <div class="col-xs-8 margin-10t">
                        <span class="text-left">Other - Meeting Minutes</span>
                    </div>
                    <div class="col-xs-12 margin-2t">
                        <input type="text" id="txtOtherMeetingMinutes" placeholder="Others Meeting Minutes" maxlength="100" class="col-xs-9 special" tabindex="19" />
                    </div>

                    <div class="col-xs-8 margin-10t">
                        <span class="text-left">Designation</span>
                    </div>
                    <div class="col-xs-12 margin-2t">
                        <input type="text" id="txtDesignation" placeholder="Designation" value="Chairman" maxlength="100" class="col-xs-9 special" tabindex="20" />
                    </div>

                    <div class="col-xs-8 margin-10t">
                        Notice of Resolution
                    </div>
                    <div class="col-xs-12 margin-2t" tabindex="21">
                        @Html.Partial("~/Areas/WO/Views/PartialContent/_DirectorsChosen.cshtml", new ViewDataDictionary { { "Id", "NoticeofResolution" }, { "selectedId", "" }, { "SearchType", "Officers" }, { "SearchRole", "GEN" }, { "WOID", @ViewBag.WOID }, { "DefaultLoad", 1 }, { "AutoSearch", 1 }, { "NatureAppoint", "9" }, { "DisplayName", "Notice of Resolution" } })
                    </div>

                    <div class="col-xs-8 margin-10t">
                        F24 / F25
                    </div>
                    <div class="col-xs-12 margin-2t" tabindex="22">
                        @Html.Partial("~/Areas/WO/Views/PartialContent/_DirectorsChosen.cshtml", new ViewDataDictionary { { "Id", "F24ByF25" }, { "selectedId", "" }, { "SearchType", "Officers" }, { "SearchRole", "GEN" }, { "WOID", @ViewBag.WOID }, { "DefaultLoad", 1 }, { "AutoSearch", 1 }, { "NatureAppoint", "9" }, { "DisplayName", "F24/F25" } })
                    </div>

                </div>

            </div>

        </div>

    </div>

</div>

<div class="row align-right margin-10t">
    <div class="col-xs-12">
        <button type="button" class="btn btn-info btnWOClose" id="btnSaveECE" tabindex="23">
            <i class="ace-icon fa fa-check bigger-110"></i>
            SAVE            
        </button>
        <button class="btn btnWOClose" type="button" id="btnClear" tabindex="24">
            <i class="ace-icon fa fa-undo bigger-110"></i>
