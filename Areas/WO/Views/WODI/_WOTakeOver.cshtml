<style>
    .ui-datepicker {
        width: 19em;
    }

    .chosen-container {
        width: auto !important;
    }
</style>

<div class="pull-right col-xs-12 no-padding-right margin-10b">
    <div class="col-xs-1 pull-right no-padding-right">
        @Html.Partial("~/Areas/WO/Views/Css1Review/_Css1Review.cshtml", new ViewDataDictionary { { "ReviewPopupHeader", "CSS1 Takeover Review" } })
    </div>
</div>

<div class="row margin-10t">

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 120px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-8 margin-10t">
                    Name of Company
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Name of Company" id="txtCompanyName" class="col-xs-8 special" tabindex="1" maxlength="50" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Registration No
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Registration No" id="txtRegistrationNo" tabindex="2" maxlength="100" class="col-xs-8 special" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    <div class="col-xs-7 no-padding-left">
                        FMG as Registered Address ?
                    </div>

                    <div class="col-xs-2">
                        <label>
                            <input type="checkbox" id="chkFMHasRegisteredAddress" class="ace" />
                            <span class="lbl" tabindex="13"></span>
                        </label>
                    </div>
                </div>

                <div class="col-xs-8 margin-10t">
                    RO Address Line1
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Address Line1" id="txtAddressLine1" class="col-xs-8 special" maxlength="100" tabindex="14" />
                </div>

                <div class="col-xs-8 margin-10t">
                    RO Address Line2
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Address Line2" id="txtAddressLine2" class="col-xs-8 special" maxlength="100" tabindex="15" />
                </div>

                <div class="col-xs-12 margin-10t">
                    RO Address Line3
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtAddressLine3" placeholder="Address Line3" class="col-xs-8 special" maxlength="100" tabindex="16" />
                </div>

                <div class="text-left col-xs-8 margin-10t">
                    RO Address Country
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-8 no-padding-left" id="ddlAddressCountry" tabindex="17">
                        <option value="-1">Select</option>
                    </select>
                </div>
                <div class="col-xs-8 margin-10t">
                    RO Address Postal Code
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Address Postal Code" id="txtAddressPostalCode" maxlength="100" class="col-xs-8 DisableCutCopyPaste" tabindex="18" />
                </div>

            </div>
        </div>

    </div>

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 20px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Class Of Share</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <select id="ddlClassOfShare" tabindex="5" class="col-xs-8">
                        <option value="-1">Select</option>
                    </select>
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Currency</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <select id="ddlCurrency" tabindex="4" class="col-xs-8">
                        <option value="-1">Select</option>
                    </select>
                </div>

                <div class="col-xs-11 margin-10t">
                    <span class="text-left">Total No Of New Shares To Be Allotted</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtAllotedShares" onkeypress="return EnterOnlyNumbers(event)" placeholder="Total No Of New Shares To Be Allotted" class="form-control col-xs-8 DisableCutCopyPaste" maxlength="30" tabindex="6" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Consideration Of Each Share</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtConsiderationOfEachShare" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Consideration Of Each Share" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="7" />
                </div>

                <div class="col-xs-11 margin-10t">
                    <span class="text-left">Amount To Be Treated As Paid On Each Share</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtPaidOnEachShare" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Amount To Be Treated As Paid On Each Share" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="8" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Total Consideration</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtTotalConsideration" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Total Consideration" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="9" />
                </div>

                <div class="col-xs-12 margin-10t">
                    Resultant Total No Of Issued Shares
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Resultant Total No Of Issued Shares" onkeypress="return EnterOnlyNumbers(event)" id="txtTotalIssuedShares" maxlength="30" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="10" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Resultant Issued Capital
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Resultant Issued Capital" id="txtResultantIssuedCapital" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="11" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Resultant Paid Up Capital
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Resultant Issued Capital" id="txtResultantPaidUpCapital" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="12" />
                </div>




            </div>

        </div>

    </div>

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 40px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-8 margin-10t">
                    Auditor Name
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Auditor Name" id="txtAuditor" maxlength="100" class="form-control col-xs-8 DisableCutCopyPaste special" tabindex="24" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Out-Going Auditor
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Out Going Auditor" id="txtOutGoingAuditor" maxlength="100" class="col-xs-8 DisableCutCopyPaste special" tabindex="25" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Date Of Next Acc To Be Laid</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtDateOfNextAccToBeLaid" placeholder="DDMMYYYY" class="col-xs-8 DisableCutCopyPaste special" onblur="javascript:validateDateDDMMYYYY(this)" onkeypress="return checkDateValidation(event)" maxlength="10" tabindex="3" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <h4 class="text-left">Signing Director</h4>
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Meeting Notice</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Notice" id="txtMeetingNotice" maxlength="100" class="col-xs-8 DisableCutCopyPaste special" tabindex="19" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Meeting Minutes</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Minutes" id="txtMeetingMinutes" maxlength="100" class="col-xs-8 DisableCutCopyPaste special" tabindex="20" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Designation</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtDesignation" placeholder="Designation" value="Chairman" maxlength="100" class="col-xs-8 special" tabindex="21" />
                </div>

                <div class="col-xs-8 margin-10t">
                    Notice of Resolution
                </div>

                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Notice of Resolution" id="txtNoticeResolution" maxlength="100" class="col-xs-8 DisableCutCopyPaste special" tabindex="22" />
                </div>

                <div class="col-xs-8 margin-10t">
                    F24 / F25
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="F24 By F25" id="txtF24ByF25" maxlength="100" class="col-xs-8 DisableCutCopyPaste special" tabindex="23" />
                </div>



                <div class="text-left col-xs-12 margin-10t" style="display: none;">
                    Share Holding Structure
                </div>
                <div class="col-xs-12 margin-2t" style="display: none;">
                    <select id="ddlShareHoldingStructure" tabindex="26" class="col-xs-8">
                        <option value="-1">Select</option>
                    </select>
                </div>

            </div>

        </div>

    </div>

</div>

<div class="row align-right margin-10t">
    <div class="col-xs-12">
        <button type="button" class="btn btn-info btnWOClose" id="btnSaveTakeOver" tabindex="27">
            <i class="ace-icon fa fa-check bigger-110"></i>
            SAVE            
        </button>
        <button class="btn btnWOClose" type="button" id="btnClear" tabindex="28">
            <i class="ace-icon fa fa-undo bigger-110"></i>
            RESET           
        </button>
    </div>
</div>


