                            </div>

                            <div class="col-xs-4">
                                <img src="~/Content/images/i_icon.png" style="cursor: pointer; width: 30px;" class="btnIndividual" id="btnIndividual_PrincipalDetails" />
                                <img src="~/Content/images/c_icon.png" style="cursor: pointer; width: 32px;" class="btnCompany" id="btnCompany_PrincipalDetails" />
                            </div>
                        </div>

                        <div class="text-left col-xs-12" style="padding-top: 10px;">
                            Email
                        </div>
                        <div class="col-xs-12">
                            <input type="text" placeholder="Email" class="col-xs-8 special" id="txtDirectorEmail_PrincipalDetails" maxlength="100" />
                        </div>

                        <div class="text-left col-xs-12" style="padding-top: 10px;">
                            Contact No
                        </div>
                        <div class="col-xs-12">
                            <input type="text" placeholder="Contact No" class="col-xs-8" id="txtDirectorContactNo_PrincipalDetails" maxlength="100" />
                        </div>

                        <div class="text-left col-xs-12" style="padding-top: 10px;">
                            Fax
                        </div>
                        <div class="col-xs-12">
                            <input type="text" placeholder="Fax" class="col-xs-8" id="txtDirectorFax_PrincipalDetails" maxlength="100" />
                        </div>
                        <div class="text-left col-xs-12" style="padding-top: 10px;">
                            Contact Person
                        </div>
                        <div class="col-xs-12">
                            <input type="text" placeholder="Contact Person" class="col-xs-8" id="txtContactPerson_PrincipalDetails" maxlength="100" />
                        </div>

                    </div>

                    <div class="col-xs-12 align-right">
                        @*<input type="hidden" value="0" id="hdnItemCounts_AuthorizedPersonFSDetails" />*@
                        <button class="btn btn-info btnWOClose" type="button" id="btnSave_PrincipalDetails">
                            <i class="ace-icon fa fa-check bigger-110"></i>
                            SAVE
       
                        </button>

                    </div>

                </div>

            </div>

        </div>
    </div>
</div>
