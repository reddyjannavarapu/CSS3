@*<script src="~/Scripts/Validations.js"></script>*@


<div class="pull-right col-xs-12 no-padding-right margin-10b">
    @Html.Partial("~/Areas/WO/Views/Css1Info/_Css1Info.cshtml", new ViewDataDictionary { { "PopupHeader", "CSS1 EGM Acquisition Disposal of Property Info" } })
</div>


<div class="row">

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 24px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Type of Transaction</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <select id="ddlTypeOfTransaction" tabindex="1" class="col-xs-9">
                        <option value="1">Acquisition of Property</option>
                        <option value="2">Disposal of Property</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Property Address Line1
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Property Address Line1" id="txtPropertyAddressLine1" class="col-xs-9 special" maxlength="100" tabindex="16" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Property Address Line2
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Property Address Line2" id="txtPropertyAddressLine2" class="col-xs-9 special" maxlength="100" tabindex="17" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Property Address Line3
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Property Address Line3" id="txtPropertyAddressLine3" maxlength="100" class="col-xs-9 special" tabindex="18" />
                </div>

                <div class="text-left col-xs-10 margin-10t">
                    Property Address Country
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlPropertyAddressCountry" tabindex="19">
                        <option value="-1">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Property Address Postal Code
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Property Address Postal Code" id="txtPropertyAddressPostalCode" maxlength="10" class="col-xs-9 DisableCutCopyPaste" tabindex="20" />
                </div>

                <div class="col-xs-12 margin-10t">
                    <span class="text-left">Name of Purchaser/Vendor</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtNameVendor" placeholder="Name of Purchaser/Vendor" class="form-control col-xs-9 special" maxlength="100" tabindex="9" />
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Consideration</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <select id="ddlConsiderationCurrency" tabindex="7" class="col-xs-3">
                        <option value="-1">Select</option>
                    </select>
                    <input type="text" id="txtConsiderationAmount" onkeypress="return AllowDecimalNumbersOnly(this,event,34,4)" maxlength="39" placeholder="Consideration Amount" class="form-control col-xs-6 DisableCutCopyPaste special" tabindex="8" />
                </div>

            </div>

        </div>

    </div>

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 183px;">

            <div class="col-xs-offset-1">

                <div class="text-left col-xs-12 margin-10t">
                    <div class="col-xs-8 no-padding-left">
                        RO as Place of Meeting
                    </div>
                    <div class="col-xs-4">
                        <label>
                            <input type="checkbox" id="chkROPlaceOfMeeting" tabindex="10" class="col-xs-9 ace" />
                            <span class="lbl"></span>
                        </label>

                    </div>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Line1
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Address Line1" id="txtMeetingAddressLine1" class="col-xs-9 special" maxlength="100" tabindex="11" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Line2
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Address Line2" id="txtMeetingAddressLine2" class="col-xs-9 special" maxlength="100" tabindex="12" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Line3
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtMeetingAddressLine3" placeholder="Meeting Address Line3" class="col-xs-9 special" maxlength="100" tabindex="13" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Country
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlMeetingAddressCountry" tabindex="14">
                        <option value="-1">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Meeting Address Postal Code
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Meeting Address Postal Code" id="txtMeetingAddressPostalCode" maxlength="10" class="col-xs-9 DisableCutCopyPaste" tabindex="15" />
                </div>

            </div>

        </div>

    </div>

    <div class="col-xs-4">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 248px;">

            <div class="col-xs-offset-1">

                <div class="col-xs-12">
                    <h4 class="text-left">Signing Director</h4>
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Meeting Notice</span>
                </div>
                <div class="col-xs-12 margin-2t" tabindex="2">
                    @Html.Partial("~/Areas/WO/Views/PartialContent/_DirectorsChosen.cshtml", new ViewDataDictionary { { "Id", "MeetingNotice" }, { "selectedId", "" }, { "SearchType", "Officers" }, { "SearchRole", "GEN" }, { "WOID", @ViewBag.WOID }, { "DefaultLoad", 1 }, { "AutoSearch", 1 }, { "NatureAppoint", "9,7" }, { "DisplayName", "Select Meeting Notice" } })
                </div>

                <div class="col-xs-8 margin-10t">
                    <span class="text-left">Meeting Minutes</span>
                </div>
                <div class="col-xs-12 margin-2t" tabindex="3">
                    @Html.Partial("~/Areas/WO/Views/PartialContent/_DirectorsChosen.cshtml", new ViewDataDictionary { { "Id", "MeetingMinutes" }, { "selectedId", "" }, { "SearchType", "MeetingMinutes" }, { "SearchRole", "GEN" }, { "WOID", @ViewBag.WOID }, { "DefaultLoad", 1 }, { "AutoSearch", 1 }, { "NatureAppoint", "9,-1,17" }, { "DisplayName", "Select Meeting Minutes" } })
                </div>

                <div class="col-xs-12 margin-10t">
                    <span class="text-left">Other - Meeting Minutes</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtOtherMeetingMinutes" placeholder="Others Meeting Minutes" maxlength="100" class="form-control col-xs-9 special" tabindex="4" />
                </div>

                <div class="col-xs-12 margin-10t">
                    <span class="text-left">Designation</span>
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" id="txtDesignation" placeholder="Designation" maxlength="100" class="form-control col-xs-9 special" tabindex="5" />
                </div>

                <div class="text-left col-xs-12 margin-10t" style="display: none;">
                    Share Holding Structure
                </div>
                <div class="col-xs-12 margin-2t" style="display: none;">
                    <select id="ddlShareHoldingStructure" tabindex="6" class="col-xs-9">
                        <option value="-1">Select</option>
                    </select>
                </div>

            </div>

        </div>
    </div>

</div>

<div style="margin-top: 25px;" class="align-right col-xs-12 no-padding-right">
    <button type="button" class="btn btn-info btnWOClose" id="btnSaveEGM" tabindex="21">
        <i class="ace-icon fa fa-check bigger-110"></i>
        SAVE            
    </button>
    <button class="btn btnWOClose" type="button" id="btnClear" tabindex="22">
        <i class="ace-icon fa fa-undo bigger-110"></i>
        RESET           
    </button>
</div>

@*<div class="row" style="margin-right: 677px; margin-top: 25px;">
</div>*@

<div id="divScripts">
    <script id="ShareHoldingStructure" type="text/html">
        <option value="{{= ID}}">{{= Name}}</option>
    </script>
    <script id="scriptCurrency" type="text/html">
        <option value="{{= CurrencyID}}">{{= CurrencyName}}</option>
    </script>
    <script id="scriptCountry" type="text/html">
        <option value="{{= CountryID}}">{{= CountryName}}</option>
    </script>
</div>
<script src="~/Areas/WO/Content/Scripts/_WOEGMAcquisitionDisposal.js"></script>

