<style>
    .chosen-single.chosen-default {
        border-radius: 0px !important;
        min-height: 29px;
        max-width: 300px !important;
        width: 250px !important;
    }

    .chosen-search:after {
        content: "" !important;
    }

    .chosen-single.chosen-single-with-deselect {
        border-radius: 0px !important;
        min-height: 29px;
        max-width: 300px !important;
        width: 250px !important;
    }

    .search-choice-close {
        background: none !important;
        margin-top: 3px;
    }

    .chosen-drop {
        max-width: 300px;
        width: 250px !important;
    }
