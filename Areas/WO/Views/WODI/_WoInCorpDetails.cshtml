<div class="col-xs-12 margin-10t no-padding">

    <div class="col-xs-4 no-padding-left">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 23px;">

            <div class="col-xs-offset-1">

                <div class="text-left col-xs-12 margin-10t">
                    Name of Company
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Name of Company" id="txtCompanyName" class="col-xs-9 special" tabindex="1" maxlength="50" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Registration No
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Registration No" id="txtRegistrationNo" tabindex="2" maxlength="100" class="col-xs-9 special" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    <span class="col-xs-8 no-padding-left">FMG Client</span>
                    <label class="col-xs-2 no-margin">
                        <input type="checkbox" name="form-field-checkbox" id="chkFMGClient" class="ace">
                        <span class="lbl" tabindex="3"></span>
                    </label>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Client Type
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlClientType" tabindex="4" disabled="">
                        <option value="">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Type of Company
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlTypeofCompanyForIncrop" tabindex="5">
                        <option value="">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Client No
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Client No" id="txtClientNo" class="col-xs-9 special" tabindex="6" maxlength="50" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Incorporation Date
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Incorporation Date" id="txtIncropDate" tabindex="7" class="col-xs-9 DisableCutCopyPaste special" onblur="javascript:validateDateDDMMYYYY(this)" onkeypress="return checkDateValidation(event)" maxlength="10" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Incorporation Country
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlIncorporationCountry" tabindex="8">
                        <option value="">Select</option>
                    </select>
                </div>

            </div>

        </div>

    </div>

    <div class="col-xs-4 no-padding">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 86px;">

            <div class="col-xs-offset-1">

                <div class="text-left col-xs-12 margin-10t">
                    Company Status
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlCompanyStatus" tabindex="9" disabled="">
                        <option value="">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    <span class="col-xs-8 no-padding-left">FMG as Registered Address ?</span>
                    <label class="col-xs-2 no-margin">
                        <input type="checkbox" name="form-field-checkbox" id="chkFMGasRegisteredAddress" class="ace">
                        <span class="lbl" tabindex="10"></span>
                    </label>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Address Line1
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Address Line1" id="txtAddressLineOne" tabindex="11" maxlength="100" class="col-xs-9 special" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Address Line2
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Address Line2" id="txtAddressLineTwo" class="col-xs-9 special" tabindex="12" maxlength="100" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Address Line3
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Address Line3" id="txtAddressLineThree" tabindex="13" maxlength="100" class="col-xs-9 special" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Address Country
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlCountryForIncorp" tabindex="14">
                        <option value="">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Address Postal Code
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Postal Code" id="txtPostalCode" pattern="^[0-9]{6}*\.[0-9]{2}$" class="col-xs-9" tabindex="15" maxlength="100" />
                </div>

            </div>

        </div>

    </div>

    <div class="col-xs-4 no-padding-right">

        <div class="col-xs-12 WoBC WoBR" style="padding-top: 20px; padding-bottom: 44px;">

            <div class="col-xs-offset-1">

                <div class="text-left col-xs-12 margin-10t">
                    Financial Year End
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Date Month" id="dateFinancialYearEnd" tabindex="16" class="col-xs-9 DisableCutCopyPaste" tabindex="12" onblur="javascript:validateDateDD_MM(this)" maxlength="15" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    1st Financial Year End
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Date Month Year" id="dateFirstFinancialYearEnd" tabindex="17" class="col-xs-9 DisableCutCopyPaste" onblur="javascript:validateDateDD_MM_YYYY(this)" maxlength="20" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Currency
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlCurrencyForIncrop" tabindex="18">
                        <option value="">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Class of Share
                </div>
                <div class="col-xs-12 margin-2t">
                    <select class="col-xs-9 no-padding-left" id="ddlShareClassForIncrop" disabled="" tabindex="19">
                        <option value="">Select</option>
                    </select>
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Paid Up Capital
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Paid up Capital" id="txtPaidupCapital" onkeypress="return AllowDecimalNumbersOnly(this,event,17,4)" tabindex="20" class="col-xs-9 DisableCutCopyPaste special" />
                </div>

                <div class="text-left col-xs-12 margin-10t">
                    Amount Paid Up Per Share
                </div>
                <div class="col-xs-12 margin-2t">
                    <input type="text" placeholder="Amount paid up per share" id="txtAmountPaidupperShare" onkeypress="return AllowDecimalNumbersOnly(this,event,17,4)" tabindex="21" value="1.0" class="col-xs-9 DisableCutCopyPaste special" />
                </div>

                <div class="col-xs-12 margin-10t">
                    <span class="text-left col-xs-12 no-padding-left">Amount Guaranteed by each Member</span>
                    <select class="col-xs-2 no-padding-left margin-10t" tabindex="22" id="ddlGuaranteedAmountCurrency">
                        <option value="">Select</option>
                    </select>
                    <input type="text" class="form-control col-xs-7 margin-10t DisableCutCopyPaste special" onkeypress="return AllowDecimalNumbersOnly(this,event,17,4)" placeholder="Guaranteed Amount" tabindex="22" id="txtGuaranteedAmount">
                </div>

            </div>

        </div>

    </div>

</div>

