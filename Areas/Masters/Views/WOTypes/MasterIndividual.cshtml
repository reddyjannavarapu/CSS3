@{
    ViewBag.Title = "Individual";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="divMatersIndividual">
    @Html.Partial("~/Areas/WO/Views/PartialContent/_CreateIndividual.cshtml")
</div>

<script src="~/Areas/Masters/Content/Scripts/MasterIndividual.js"></script>

<div class="margin-5t"></div>

<div class="tableGrid dataTables_wrapper form-inline">
    <div id="divSearch" class="row widget-main margin-10l">

        <div class="pull-left">
            Name 
                  <input type="text" id="txtNameOfThecompany" maxlength="50" tabindex="19" class="special" />
            Sort By 
                <select id="ddlOrderBy" tabindex="20">
                    <option value="Name ASC" selected="selected">Name ASC</option>
                    <option value="Name DESC">Name DESC</option>
                </select>
        </div>
        <div class="pull-left">
            Page Size
            <select id="ddlPageSize" tabindex="21">
                <option value="5">5</option>
                <option value="10" selected="selected">10</option>
                <option value="20">20</option>
                <option value="50">50</option>
                <option value="100">100</option>
            </select>
        </div>

        <div>
            <button id="btnSearchIndividual" class="btn btn-info" type="button" style="margin-left: 33px;" tabindex="22">
                <i class="ace-icon fa fa-search bigger-110"></i>
                SEARCH
            </button>
        </div>

    </div>
</div>
<div class="tableGrid dataTables_wrapper form-inline">
    <div id="divIndividualList">
        <table id="tblIndividualData" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Name
                    </th>
                    <th>Nationality
                    </th>
                    <th>Singapore PR
                    </th>
                    <th>Passport
                    </th>
                    <th>Passport/Fin Expiry Date</th>
                    <th>NRIC\FIN No
                    </th>
                    <th>ACCPAC Code</th>
                    <th>Address Line 1
                    </th>
                    <th>Country
                    </th>
                    <th>Postal Code
                    </th>

                    <th>Actions
                    </th>
                </tr>
            </thead>
            <tbody id="trData">
            </tbody>
        </table>

        <div id="divIndividualData" class="row" style="display: none;">
            <table class="table table-striped table-bordered table-hover">
                <tbody>
                    <tr>
                        <td colspan="5">
                            <div id="noIndividualData">
                                No data found.
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

        <div class="row" id="divPagination" style="display: none">
            <div class="col-xs-6 margin-10t"><span id="listName"></span>&nbsp;<span id="listCount" class="badge"></span></div>
            <div id="numericcontainer" class="dataTables_paginate paging_bootstrap">
                <div>
                    <ul id="pagination-digg" class="pagination">
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <script id="IndividualTemplate" type="text/html">
        <tr>
            <td>{{= Name}}
            </td>
            <td>{{= NationalityName}}
            </td>
            <td>{{if SingaporePR=="True" }}
                    <span class="label label-sm label-success">Yes</span>
                {{else}} 
                    <span class="label label-sm label-warning">No</span>
                {{/if}}
            </td>
            <td>{{= Passport}}
            </td>
            <td>{{= NRICExpiryDateInList}}
            </td>
            <td>{{= NricFinNo}}
            </td>

            <td>{{= AccPacCode}}</td>
            <td>{{= AddressLine1}}
            </td>
            <td>{{= CountryName}}
            </td>
            <td>{{= PostalCode}}
            </td>
            <td>
                <div class="action-buttons">
                    <a class="blue aView" href="javascript:void(0)" indvid="{{= ID}}" name="{{= Name}}" nationalityname="{{= NationalityName}}" singaporepr="{{= SingaporePR}}" accpaccode="{{= AccPacCode}}"
                        passport="{{= Passport}}" nricexpirydate="{{= NRICExpiryDate}}" dateofbirth="{{= DateOfBirth}}" nricfinno="{{= NricFinNo}}" addressline1="{{= AddressLine1}}" addressline2="{{= AddressLine2}}"
                        addressline3="{{= AddressLine3}}" countryname="{{= CountryName}}" postalcode="{{= PostalCode}}" occupation="{{= Occupation}}" email="{{= Email}}"
                        phone="{{= ContactNo}}" fax="{{= Fax}}">
                        <i class=" ace-icon fa fa-search-plus bigger-130"></i>
                    </a>
                    <a class="aEdit green" href="javascript:void(0)" indvid="{{= ID}}" name="{{= Name}}" nationality="{{= Nationality}}" singaporepr="{{= SingaporePR}}" accpaccode="{{= AccPacCode}}"
                        passport="{{= Passport}}" nricexpirydate="{{= NRICExpiryDate}}" dateofbirth="{{= DateOfBirth}}" nricfinno="{{= NricFinNo}}" addressline1="{{= AddressLine1}}" addressline2="{{= AddressLine2}}"
                        addressline3="{{= AddressLine3}}" country="{{= Country}}" postalcode="{{= PostalCode}}" occupation="{{= Occupation}}" email="{{= Email}}"
                        phone="{{= ContactNo}}" fax="{{= Fax}}">
                        <i class="ace-icon fa fa-pencil bigger-130"></i>
                    </a>
                </div>
            </td>
        </tr>
    </script>

    <div class="clear"></div>
</div>

<div id="modal-form" class="modal" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="blue bigger">Individual Information</h4>
            </div>

            <div class="modal-body">
                <table style="width: 70%">
                    <tr>
                        <td>Name
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnName" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Nationality
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnNationality" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Singapore PR?
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnSingaporePR" style="margin: 6px;"></label>
                        </td>
                    </tr>

                    <tr>
                        <td>Date Of Birth 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnDOB" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>NRIC\FIN No.
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnNricFinNo" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Passport
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnPassport" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Passport/Fin Expiry Date
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnNRICExpiryDate" style="margin: 6px;"></label>
                        </td>
                    </tr>

                    <tr>
                        <td>Address Line1 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnAddressLine1" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Address Line2 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnAddressLine2" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Address Line3
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnAddressLine3" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Country 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnCountry" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Postal Code 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnPostalCode" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Occupation
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnOccupation" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Email
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnEmail" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Contact No 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnPhoneNo" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>Fax
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnFax" style="margin: 6px;"></label>
                        </td>
                    </tr>
                    <tr>
                        <td>ACCPAC Code 
                        </td>
                        <td>:
                        </td>
                        <td>
                            <label id="spnAccPacCode" style="margin: 6px;"></label>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
